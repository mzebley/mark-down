{
  "name": "@mzebley/mark-down",
  "version": "1.2.2",
  "description": "markâ†“ core runtime and shared utilities",
  "type": "module",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./slug": {
      "types": "./dist/slug.d.ts",
      "import": "./dist/slug.js",
      "default": "./dist/slug.js"
    },
    "./browser": {
      "types": "./dist/browser.d.ts",
      "import": "./dist/browser.js",
      "default": "./dist/browser.js"
    },
    "./inline": {
      "types": "./dist/inline.d.ts",
      "import": "./dist/inline.js",
      "default": "./dist/inline.js"
    },
    "./angular": {
      "types": "./dist/angular/index.d.ts",
      "import": "./dist/angular/index.js",
      "default": "./dist/angular/index.js"
    }
  },
  "scripts": {
    "build": "tsup --config tsup.config.ts",
    "dev": "tsup --config tsup.config.ts --watch"
  },
  "dependencies": {
    "marked": "^11.1.0",
    "yaml": "^2.4.1"
  },
  "sideEffects": false
}
