{
  "name": "@mzebley/mark-down",
  "version": "0.1.0",
  "description": "markâ†“ core runtime and shared utilities",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./slug": {
      "types": "./dist/slug.d.ts",
      "import": "./dist/slug.mjs",
      "require": "./dist/slug.cjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/slug.ts --dts --format esm,cjs",
    "dev": "tsup src/index.ts --watch"
  },
  "dependencies": {
    "gray-matter": "^4.0.3",
    "marked": "^11.1.0"
  }
}
